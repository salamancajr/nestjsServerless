    "ts-loader": "4.5.0",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.1.0",
    "webpack-node-externals": "^2.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
